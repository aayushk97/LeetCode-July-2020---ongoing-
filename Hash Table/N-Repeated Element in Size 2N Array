class Solution {
public:
    int repeatedNTimes(vector<int>& A) {
        int n = A.size();
        unordered_map<int, int> numbers;
        
        for(int i = 0; i < n; i++){
            numbers[A[i]]++;
            
            if(numbers[A[i]] >1) return A[i];
        }
        
        return -1;
    }
};
